    "@openzeppelin/contracts": "^4.6.0",
    "@supercharge/promise-pool": "^2.1.0",
    "@types/node": "^17.0.34",
    "bignumber.js": "^9.0.2",
    "env-cmd": "^10.1.0",
    "ethereumjs-util": "^7.1.4",
    "ethers": "^5.6.6",
    "npm": "^8.10.0",
    "prettier": "^2.6.2",
